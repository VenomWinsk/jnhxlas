CA,CAAA","sourcesContent":["<template>\n    <div class=\"sider-menu\">\n        <Sider\n            ref=\"side1\"\n            hide-trigger\n            collapsible\n            :collapsed-width=\"78\"\n            :value=\"isCollapsed\"\n        >\n            <div @click=\"collapsedSider\" style=\"cursor: pointer\">\n                <Icon :class=\"rotateIcon\" type=\"md-menu\" size=\"24\"></Icon>\n            </div>\n            <Menu\n                :active-name=\"activeName\"\n                width=\"auto\"\n                :class=\"menuitemClasses\"\n                @on-select=\"toRoute\"\n            >\n                <MenuItem\n                    :name=\"menu.nameEn\"\n                    v-for=\"menu in menus\"\n                    :key=\"menu.nameEn\"\n                >\n                    <Icon :type=\"menu.icon\"></Icon>\n                    <span>{{ menu.nameCn }}</span>\n                </MenuItem>\n                <!-- <MenuItem name=\"role\">\n                    <Icon type=\"ios-search\"></Icon>\n                    <span>角色管理</span>\n                </MenuItem>\n                <MenuItem name=\"might\">\n                    <Icon type=\"ios-settings\"></Icon>\n                    <span>权限管理</span>\n                </MenuItem> -->\n            </Menu>\n        </Sider>\n    </div>\n</template>\n<script>\nexport default {\n    name: 'slider-menu',\n    props: {\n        moduleName: null,\n    },\n    data() {\n        return {\n            isCollapsed: false,\n            activeName: null,\n        };\n    },\n    computed: {\n        rotateIcon() {\n            return ['menu-icon', this.isCollapsed ? 'rotate-icon' : ''];\n        },\n        menuitemClasses() {\n            return ['menu-item', this.isCollapsed ? 'collapsed-menu' : ''];\n        },\n        menus() {\n            return this.$store.state.userData.menus.find(\n                (list) => list.nameEn === this.moduleName\n            ).children;\n        },\n    },\n    methods: {\n        collapsedSider() {\n            this.$emit('input', !this.isCollapsed);\n            // this.$refs.side1.toggleCollapse();\n            this.isCollapsed = !this.isCollapsed;\n        },\n        // 路由跳转\n        toRoute(name) {\n            this.activeName = name;\n            this.$router.push({ name });\n        },\n    },\n    mounted() {\n        // console.log(this.$route);\n        this.activeName = this.$route.name;\n    },\n};\n</script>\n<style lang=\"scss\">\n@import './sider-menu.scss';\n</style>\n"],"sourceRoot":"src/components/slider"}]}