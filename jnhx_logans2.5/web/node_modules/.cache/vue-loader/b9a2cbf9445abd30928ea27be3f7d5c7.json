>\n\t\t\t\t</div>\n\t\t\t</div>\n\t\t</Drawer>\n\t\t<!-- 详情modal -->\n\t\t<Modal v-model=\"detailVisible\" title=\"详情统计\" width=\"600\">\n\t\t\t<my-table\n\t\t\t\t:column=\"detailTableConfig.column\"\n\t\t\t\t:data=\"detailTableConfig.data\"\n\t\t\t\t:tools=\"detailTableConfig.tools\"\n                :total=\"detailTableConfig.total\"\n                :page=\"detailTableConfig.page\"\n                :tableLoading=\"detailTableConfig.loading\"\n\t\t\t></my-table>\n\t\t</Modal>\n\t\t<!-- 缓存modal -->\n\t\t<!-- <Modal v-model=\"cacheVisible\" title=\"缓存书签\" width=\"500\">\n\t\t\t<div class=\"top\">\n\t\t\t\t<label for=\"name\" style=\"width:80px\">缓存名称：</label>\n\t\t\t\t<Input\n\t\t\t\t\tv-model=\"cache_name\"\n\t\t\t\t\tplaceholder=\"输入缓存名称\"\n\t\t\t\t\tstyle=\"width: calc(100% - 80px)\"\n\t\t\t\t></Input>\n\t\t\t</div>\n\t\t\t<div slot=\"footer\">\n\t\t\t\t<Button type=\"default\" @click=\"cacheVisible = false;\">取消</Button>\n\t\t\t\t<Button type=\"primary\" @click=\"goCache\">确定</Button>\n\t\t\t</div>\n\t\t</Modal> -->\n\t</div>\n</template>\n<script>\nimport $ from 'jquery';\nimport rangeTree from '@/components/rangeTree/range-tree.vue';\nimport { getLogs } from '@/api/log';\nimport folderOpen from '@/assets/main/folder-open.png';\nimport folderClose from '@/assets/main/folder-close.png';\nimport avatar from '@/assets/main/tag.png';\nimport noContent from '@/assets/main/no-content.png';\nimport CheckFields from '@/components/checkFields/checkFields.vue';\nimport MyTable from '@/components/mytable/my-table.vue';\nimport CollectTree from '@/components/collectTree/collect-tree.vue';\nimport { getUnit, getUnitByUnit } from '@/api/setting';\nimport { getTreeById, getCollectById, download,delCollect } from '@/api/search';\nimport { setTreeByGrokeData } from '@/utils/util';\nimport { getFieldsByType } from '@/api/operation';\nexport default {\n\tcomponents: { rangeTree, CheckFields, MyTable, CollectTree },\n\tname: 'search',\n\tdata() {\n\t\treturn {\n\t\t\ttransform: 0, // 滚动距离\n\t\t\ttreeNodes: [],\n\t\t\tparamsForm: {},\n\t\t\tCheckFieldsVisible: false,\n\t\t\tfields: [], // 所有字段\n\t\t\teffectFields: [], // 有效字段\n\t\t\tcollectVisible: false, // 收藏modal的显示隐藏\n\t\t\tcollectListVisible: false, // 书签栏\n\t\t\tparamCardVisible: true, // 条件栏的显示隐藏\n\t\t\tcollectParam: {\n\t\t\t\tname: '',\n\t\t\t},\n\t\t\ttableConfig: {\n\t\t\t\tcolumn: [],\n\t\t\t\tdata: [],\n\t\t\t\tpage: 1,\n\t\t\t\ttotal: 0,\n\t\t\t\tloading: false,\n\t\t\t},\n\t\t\ttimeStart: 0,\n\t\t\ttimeEnd: 0,\n\t\t\tavatar,\n\t\t\tnoContent,\n\t\t\t// 境内外参数\n\t\t\tarea: 'all',\n\t\t\t/**  详情类型选择\n\t\t\t * commment     普通查询\n\t\t\t * unamecounty  邮箱名/用户名统计\n\t\t\t * req_ipcount  ip统计\n\t\t\t * countrycount 国家统计\n\t\t\t * citycount    城市统计\n\t\t\t */\n\t\t\tqtype: 'commment',\n\t\t\tsearchTab: [],\n\t\t\tcurrentTabId: 0,\n\t\t\tdetail_column: {\n\t\t\t\tunamecounty: [\n\t\t\t\t\t{\n\t\t\t\t\t\ttitle: '用户名',\n\t\t\t\t\t\tkey: 'username',\n\t\t\t\t\t\talign: 'center',\n\t\t\t\t\t},\n\t\t\t\t],\n\t\t\t\treq_ipcount: [\n\t\t\t\t\t{\n\t\t\t\t\t\ttitle: '请求ip',\n\t\t\t\t\t\tkey: 'req_ip',\n\t\t\t\t\t},\n\t\t\t\t\t{\n\t\t\t\t\t\ttitle: '国家',\n\t\t\t\t\t\tkey: 'country',\n\t\t\t\t\t\talign: 'center',\n\t\t\t\t\t},\n\t\t\t\t\t{\n\t\t\t\t\t\ttitle: '城市',\n\t\t\t\t\t\tkey: 'city',\n\t\t\t\t\t\talign: 'center',\n\t\t\t\t\t},\n\t\t\t\t],\n\t\t\t\tcitycount: [\n\t\t\t\t\t{\n\t\t\t\t\t\ttitle: '城市',\n\t\t\t\t\t\tkey: 'city',\n\t\t\t\t\t\talign: 'center',\n\t\t\t\t\t},\n\t\t\t\t],\n\t\t\t\tcountrycount: [\n\t\t\t\t\t{\n\t\t\t\t\t\ttitle: '国家',\n\t\t\t\t\t\tkey: 'country',\n\t\t\t\t\t\talign: 'center',\n\t\t\t\t\t},\n\t\t\t\t],\n\t\t\t},\n\t\t\tdetailVisible: false,\n\t\t\tdetailTableConfig: {\n\t\t\t\tcolumn: [],\n\t\t\t\tdata: [],\n\t\t\t\ttools: ['export'],\n                page: 1,\n\t\t\t\ttotal: 0,\n\t\t\t\tloading: false,\n\t\t\t},\n\t\t\tcurrentStr: '',\n\t\t\t//* start */\n\t\t\tunitData: [], // 所有单位数据\n\t\t\tproData: [], // 某个单位下的数据信息\n\t\t\ttreeForm: {\n\t\t\t\tunit: 0,\n\t\t\t\ttype: 1,\n\t\t\t\tpro: 0,\n\t\t\t},\n\t\t\tselectNode: {},\n\t\t\t// 是否是ip\n\t\t\tis_ip:1,\n\t\t\t// 收藏列表\n\t\t\tcollectList:[],\n\t\t\tcurNode:null,\n\t\t\tcurMark:null,\n\t\t\tcacheVisible:false,\n\t\t\tcache_name:\"\",\n\t\t};\n\t},\n\tcreated(){\n\t\t// 获取国家和城市数据\n\t},\n\tmounted() {\n\t\tthis.initPage();\n\t\t// this.getTreeNodes(this.treeForm);\n\t\t// this.getFieldsByObjectId('111');\n\t\t// this.getLogs();\n\t\tsetTimeout(() => {\n\t\t    this.calcHeight();\n\t\t}, 500);\n\t\tif (sessionStorage.getItem('searchTab')) {\n\t\t    this.searchTab = JSON.parse(sessionStorage.getItem('searchTab'));\n\t\t}\n\t},\n\tcomputed: {\n\t\telapsedTime() {\n\t\t\tif (this.tableConfig.loading) {\n\t\t\t\treturn '...';\n\t\t\t} else {\n\t\t\t\treturn this.timeEnd - this.timeStart;\n\t\t\t}\n\t\t},\n\t\tgetPlaceholder() {\n\t\t\treturn function(field){\n\t\t\t\tif(field.display_type === 'input'){\n\t\t\t\t\tlet str = field.is_many?',可多值以;隔开':'';\n\t\t\t\t\treturn `请输入${field.field_cname}${str}`;\n\t\t\t\t} else {\n\t\t\t\t\treturn `请输入${field.field_cname}`;\n\t\t\t\t}\n\t\t\t}\n\t\t}\n\t},\n\tmethods: {\n\t\t// 获取所有单位数据\n\t\tgetUnit() {\n\t\t\treturn new Promise((resolve) => {\n\t\t\t\tgetUnit().then((res) => {\n\t\t\t\t\tresolve(res);\n\t\t\t\t});\n\t\t\t});\n\t\t},\n\t\tsetNodes(nodes) {\n\t\t\treturn nodes.map((list) => {\n\t\t\t\tif (list.children && list.children.length > 0) {\n\t\t\t\t\tlist.children = this.setNodes(list.children);\n\t\t\t\t\treturn {\n\t\t\t\t\t\t...list,\n\t\t\t\t\t\topen: true,\n\t\t\t\t\t\ticon: folderClose,\n\t\t\t\t\t\ticonOpen: folderOpen,\n\t\t\t\t\t\ticonClose: folderClose,\n\t\t\t\t\t};\n\t\t\t\t} else {\n\t\t\t\t\treturn {\n\t\t\t\t\t\t...list,\n\t\t\t\t\t\topen: true,\n\t\t\t\t\t\ticon: folderClose,\n\t\t\t\t\t\ticonOpen: folderOpen,\n\t\t\t\t\t\ticonClose: folderClose,\n\t\t\t\t\t};\n\t\t\t\t}\n\t\t\t});\n\t\t},\n\t\t// 获取日志数据\n\t\tgetLogs(params,bol) {\n\t\t\tif(this.currentTabId === 0){\n\t\t\t\tlet data = {\n\t\t\t\t\ttabId:0,\n\t\t\t\t\tparams,\n\t\t\t\t}\n\t\t\t\tsessionStorage.setItem('mainTab',JSON.stringify(data))\n\t\t\t}\n\t\t\tif(bol){\n\t\t\t\tthis.detailTableConfig.loading = true;\n\t\t\t\treturn new Promise((resolve) => {\n\t\t\t\t\tgetLogs(params).then((res) => {\n\t\t\t\t\t\tthis.detailTableConfig.total = res.count;\n\t\t\t\t\t\tresolve(res.data);\n\t\t\t\t\t\tthis.detailTableConfig.loading = false;\n\t\t\t\t\t});\n\t\t\t\t});\n\t\t\t} else {\n\t\t\t\tthis.tableConfig.loading = true;\n\t\t\t\treturn new Promise((resolve) => {\n\t\t\t\t\tthis.timeStart = new Date().getTime();\n\t\t\t\t\tgetLogs(params).then((res) => {\n\t\t\t\t\t\tthis.tableConfig.total = res.count;\n\t\t\t\t\t\tresolve(res.data);\n\t\t\t\t\t\tthis.timeEnd = new Date().getTime();\n\t\t\t\t\t\tthis.tableConfig.loading = false;\n\t\t\t\t\t});\n\t\t\t\t});\n\t\t\t}\n\t\t},\n\t\t// 开启字段设置\n\t\topenSetting() {\n\t\t\tthis.CheckFieldsVisible = true;\n\t\t},\n\t\t// 获取选中的条件字段\n\t\tgetEffectFields(data) {\n\t\t\tthis.CheckFieldsVisible = false;\n\t\t\tthis.setForm(data);\n\t\t\tthis.effectFields = data;\n\t\t\tthis.$nextTick(() => {\n\t\t\t\tthis.calcHeight();\n\t\t\t});\n\t\t},\n\t\t// 根据effectFields设置form\n\t\tsetForm(effectFields) {\n\t\t\tthis.paramsForm = {};\n\t\t\teffectFields.forEach((field) => {\n\t\t\t\tif (\n\t\t\t\t\t!Object.prototype.hasOwnProperty.call(\n\t\t\t\t\t\tthis.paramsForm,\n\t\t\t\t\t\tfield.field_ename\n\t\t\t\t\t)\n\t\t\t\t) {\n\t\t\t\t\tthis.$set(this.paramsForm,field.field_ename,\"\");\n\t\t\t\t}\n\t\t\t});\n\t\t},\n\t\t// 开启收藏夹\n\t\topenCollect() {\n\t\t\tthis.collectVisible = true;\n\t\t\tthis.collectParam.name = \"\";\n\t\t},\n\t\t// 显示/隐藏 条件栏\n\t\tgoHidden() {\n\t\t\tthis.paramCardVisible = !this.paramCardVisible;\n\t\t\tthis.$nextTick(() => {\n\t\t\t\tthis.calcHeight();\n\t\t\t});\n\t\t},\n\t\t// 切换视图\n\t\tchangeViewModel() {\n\t\t\tthis.$nextTick(() => {\n\t\t\t\tthis.calcHeight();\n\t\t\t});\n\t\t},\n\t\t// 计算高度\n\t\tcalcHeight() {\n\t\t\tlet paramHeight = 0;\n\t\t\tif ($('#paramRef').length > 0) {\n\t\t\t\tparamHeight =\n\t\t\t\t\t$('#paramRef').height() +\n\t\t\t\t\t16 +\n\t\t\t\t\tparseInt($('#paramRef').css('marginBottom'));\n\t\t\t}\n\t\t\t$('.search #table_main').css(\n\t\t\t\t'max-height',\n\t\t\t\t`calc(100vh - 234px - ${paramHeight}px)`\n\t\t\t);\n\t\t},\n\t\t// 添加检索实例\n\t\taddTab(tab) {\n\t\t\tthis.searchTab = this.searchTab.filter(list => list.tabId !== tab.tabId);\n\t\t\tthis.searchTab.push(tab);\n\t\t\tthis.currentTabId = tab.tabId;\n\t\t\tthis.saveTab();\n\t\t},\n\t\tdelTab(event,tabId) {\n\t\t\tevent.stopPropagation();\n\t\t\tthis.searchTab = this.searchTab.filter(\n\t\t\t\t(tab) => tab.tabId !== tabId\n\t\t\t);\n\t\t\tif (tabId === this.currentTabId) {\n\t\t\t\tthis.currentTabId = 0;\n\t\t\t}\n\t\t\tthis.saveTab();\n\t\t},\n\t\tsaveTab() {\n\t\t\tsessionStorage.setItem('searchTab', JSON.stringify(this.searchTab));\n\t\t},\n\t\tasync startSearch() {\n\t\t\t// this.addTab();\n            this.tableConfig.total = 0;\n\t\t\tthis.tableConfig.page = 1;\n\t\t\tthis.tableConfig.data = [];\n            this.tableConfig.data = await this.getLogs(this.getQueryParam());\n\t\t},\n\t\ttabClick(tab) {\n\t\t\tthis.currentTabId = tab === 0?tab:tab.tabId;\n\t\t\tif(this.currentTabId === 0){\n\t\t\t\tlet params = JSON.parse(sessionStorage.getItem('mainTab'));\n\t\t\t\tthis.paramBack(params.params,async () => {\n\t\t\t\t\tthis.collectListVisible = false;\n\t\t\t\t\tthis.tableConfig.data = await this.getLogs(this.getQueryParam());\n\t\t\t\t});\n\t\t\t} else {\n\t\t\t\tthis.paramBack(JSON.parse(tab.mark_condations),async () => {\n\t\t\t\t\tthis.collectListVisible = false;\n\t\t\t\t\tthis.tableConfig.data = await this.getLogs(this.getQueryParam());\n\t\t\t\t});\n\t\t\t}\n\t\t},\n\t\t// 详情点击\n\t\tasync detailClick(str) {\n\t\t\tthis.detailVisible = true;\n\t\t\tthis.qtype = str;\n\t\t\tthis.detailTableConfig.column = this.detail_column[str];\n\t\t\tthis.detailTableConfig.data = await this.getLogs(this.getQueryParam(),true);\n\t\t},\n\t\t// 获取任务根据选择的单位\n\t\tgetPro(unitId) {\n\t\t\treturn new Promise((resolve) => {\n\t\t\t\tgetUnitByUnit(unitId).then((res) => {\n\t\t\t\t\tif(res.length > 0){\n\t\t\t\t\t\tresolve(res);\n\t\t\t\t\t} else {\n\t\t\t\t\t\tthis.$Message.warning(\"未查询到任务数据\");\n\t\t\t\t\t\tresolve([])\n\t\t\t\t\t}\n\t\t\t\t});\n\t\t\t});\n\t\t},\n\t\t// 获取 树节点 数据 可根据 单位id 和 任务id\n\t\tgetNodes() {\n\t\t\treturn new Promise((resolve) => {\n\t\t\t\tif (this.treeForm.type === 1) {\n\t\t\t\t\tgetTreeById(this.treeForm.unit, this.treeForm.pro).then(\n\t\t\t\t\t\t(res) => {\n                            if(res.data.length > 0){\n                                resolve(res.data);\n                            } else {\n                                this.$Message.info(\"未查询到groke数据\");\n                                resolve([]);\n                            }\n\t\t\t\t\t\t}\n\t\t\t\t\t);\n\t\t\t\t} else {\n\t\t\t\t\tgetTreeById(this.treeForm.unit).then((res) => {\n\t\t\t\t\t\tif(res.data.length > 0){\n                                resolve(res.data);\n                        } else {\n                            this.$Message.info(\"未查询到groke数据\");\n                            resolve([]);\n                        }\n\t\t\t\t\t});\n\t\t\t\t}\n\t\t\t});\n\t\t},\n\t\t// 获取字段数据\n\t\tgetFields() {\n\t\t\treturn new Promise((resolve) => {\n\t\t\t\tgetFieldsByType(this.selectNode.idArr).then((res) => {\n\t\t\t\t\tthis.fields = res.filter(\n\t\t\t\t\t\t(list) => list.field_ename !== 'sourcelog'\n\t\t\t\t\t);\n\t\t\t\t\tthis.tableConfig.column = this.fields.map((list) => {\n\t\t\t\t\t\treturn {\n\t\t\t\t\t\t\ttitle: list.field_cname,\n\t\t\t\t\t\t\tkey: list.field_ename,\n\t\t\t\t\t\t};\n\t\t\t\t\t});\n\t\t\t\t\t// 取第一个\n\t\t\t\t\tthis.effectFields = [\n\t\t\t\t\t\t...this.effectFields,\n\t\t\t\t\t\t...this.fields.filter((list, index) => index < 5),\n\t\t\t\t\t];\n\t\t\t\t\tthis.setForm(this.effectFields);\n                    resolve()\n\t\t\t\t});\n\t\t\t});\n\t\t},\n\t\t// 整合一个页面初始化\n\t\tasync initPage() {\n\t\t\t// 1. 请求所有单位信息 并设置默认单位\n\t\t\tthis.unitData = await this.getUnit();\n\t\t\tthis.treeForm.unit = this.unitData[0].unit_id;\n\t\t\t// 2. 请求所有选择单位的任务信息（任务范围模式下，并设置默认任务）\n\t\t\tif (this.treeForm.type === 1) {\n\t\t\t\tthis.proData = await this.getPro(this.treeForm.unit);\n\t\t\t\tthis.treeForm.pro = this.proData[0].pro_id;\n\t\t\t}\n\t\t\t// 3. 请求数据节点信息 并设置默认选择\n\t\t\tthis.treeNodes = this.setNodes(\n\t\t\t\tsetTreeByGrokeData(await this.getNodes())\n\t\t\t);\n\t\t\tthis.$refs.rangeTree.initTree(this.treeNodes);\n\t\t\tthis.selectNode = this.$refs.rangeTree.RTree.getNodes()[0];\n\t\t\tthis.$refs.rangeTree.RTree.selectNode(this.selectNode);\n\t\t\t// 4. 根据选择的节点信息查询字段条件数据\n\t\t\tawait this.getFields();\n\t\t\t// 5. 根据字段条件数据 查询日志信息\n\t\t\tthis.tableConfig.data = await this.getLogs(this.getQueryParam());\n\t\t},\n\t\t// 设置参数\n\t\tgetQueryParam() {\n\t\t\tlet param = {\n\t\t\t\tunit_id: this.treeForm.unit,\n\t\t\t\tproject_id: this.treeForm.type ? this.treeForm.pro : null,\n\t\t\t\trule_id: this.selectNode.idArr[2] || null,\n\t\t\t\trulegroup_id: this.selectNode.idArr[1] || null,\n\t\t\t\tanalyse_id: this.selectNode.idArr[0] || null,\n\t\t\t\toffset: this.tableConfig.page,\n\t\t\t\tqtype: this.qtype,\n\t\t\t\tarea: this.area,\n\t\t\t};\n\t\t\tlet filterParam = {};\n            for (const key in this.paramsForm) {\n                switch(this.getDisplayType(key)){\n                    case 'date':\n                        param[key] = this.setTimeComponent(key);\n                        break;\n                    case 'time':\n                        param[key] = this.setTimeComponent(key);\n                        break;\n                    default:\n                        param[key] = this.paramsForm[key];\n                        break\n                }\n            }\n\t\t\tfor (const key in param) {\n\t\t\t\tif (param[key] !== null) {\n\t\t\t\t\tfilterParam[key] = param[key];\n\t\t\t\t}\n\t\t\t}\n\t\t\tif(filterParam.hasOwnProperty('req_ip')){\n\t\t\t\tfilterParam['is_ip'] = this.is_ip;\n\t\t\t}\n\t\t\treturn filterParam;\n\t\t},\n        // 获取字段 displayType\n        getDisplayType(ename){\n            return this.fields.find(list => list.field_ename === ename).display_type;\n        },\n\t\t// 处理时间类型组件\n\t\tsetTimeComponent(key){\n\t\t\tif(this.paramsForm[key].every(val => val !== \"\" && val !== null)){\n\t\t\t\treturn this.paramsForm[key].join(';');\n\t\t\t} else {\n\t\t\t\treturn null;\n\t\t\t}\n\t\t},\n\t\t// 改变事件\n\t\tasync unitChange(val) {\n\t\t\tthis.refreshParam();\n\t\t\tif (this.treeForm.type === 1) {\n\t\t\t\tthis.proData = await this.getPro(this.treeForm.unit);\n\t\t\t\tif(this.proData.length > 0){\n\t\t\t\t\tthis.treeForm.pro = this.proData[0].pro_id;\n\t\t\t\t}\n\t\t\t}\n\t\t\tthis.treeNodes = this.setNodes(\n\t\t\t\tsetTreeByGrokeData(await this.getNodes())\n\t\t\t);\n\t\t\tthis.$refs.rangeTree.initTree(this.treeNodes);\n\t\t\tthis.selectNode = this.$refs.rangeTree.RTree.getNodes()[0];\n\t\t\tthis.$refs.rangeTree.RTree.selectNode(this.selectNode);\n\t\t\tawait this.getFields();\n\t\t\tthis.tableConfig.data = await this.getLogs(this.getQueryParam());\n\t\t},\n\t\tasync typeChange(val) {\n            this.refreshParam();\n\t\t\tif (val === 1) {\n\t\t\t\tthis.proData = await this.getPro(this.treeForm.unit);\n\t\t\t\tthis.treeForm.pro = this.proData[0].pro_id;\n\t\t\t}\n\t\t\tthis.treeNodes = this.setNodes(\n\t\t\t\tsetTreeByGrokeData(await this.getNodes())\n\t\t\t);\n\t\t\tthis.$refs.rangeTree.initTree(this.treeNodes);\n\t\t\tthis.selectNode = this.$refs.rangeTree.RTree.getNodes()[0];\n\t\t\tthis.$refs.rangeTree.RTree.selectNode(this.selectNode);\n\t\t\tawait this.getFields();\n\t\t\tthis.tableConfig.data = await this.getLogs(this.getQueryParam());\n\t\t},\n\t\tasync proChange(val) {\n\t\t\tthis.refreshParam();\n\t\t\tthis.treeNodes = this.setNodes(\n\t\t\t\tsetTreeByGrokeData(await this.getNodes())\n\t\t\t);\n\t\t\tthis.$refs.rangeTree.initTree(this.treeNodes);\n\t\t\tthis.selectNode = this.$refs.rangeTree.RTree.getNodes()[0];\n\t\t\tthis.$refs.rangeTree.RTree.selectNode(this.selectNode);\n\t\t\tawait this.getFields();\n\t\t\tthis.tableConfig.data = await this.getLogs(this.getQueryParam());\n\t\t},\n\t\tasync nodeClick(treeNode) {\n\t\t\tthis.refreshParam();\n\t\t\tthis.selectNode = treeNode;\n\t\t\tthis.$refs.rangeTree.RTree.selectNode(this.selectNode);\n\t\t\tawait this.getFields();\n\t\t\tthis.tableConfig.data = await this.getLogs(this.getQueryParam());\n\t\t},\n\t\tasync pageChange(page) {\n\t\t\tthis.tableConfig.page = page;\n\t\t\tthis.tableConfig.data = await this.getLogs(this.getQueryParam());\n\t\t},\n\t\tasync areaChange() {\n            this.tableConfig.total = 0;\n\t\t\tthis.tableConfig.page = 1;\n\t\t\tthis.tableConfig.data = [];\n\t\t\tthis.tableConfig.data = await this.getLogs(this.getQueryParam());\n\t\t},\n\t\t// 清洗参数\n\t\trefreshParam() {\n\t\t\tthis.tableConfig.total = 0;\n\t\t\tthis.tableConfig.page = 1;\n\t\t\tthis.tableConfig.data = [];\n\t\t\tthis.tableConfig.column = [];\n\t\t\tthis.field = [];\n\t\t\tthis.effectFields = [];\n\t\t},\n\t\t// 重置表单\n\t\tresetParam(){\n\t\t\tfor (const key in this.paramsForm) {\n\t\t\t\tif(this.getDisplayType(key) === 'time' || this.getDisplayType(key) === 'date'){\n\t\t\t\t\tthis.$set(this.paramsForm,key,[\"\",\"\"]);\n\t\t\t\t} else {\n\t\t\t\t\tthis.$set(this.paramsForm,key,\"\");\n\t\t\t\t}\n\t\t\t}\n\t\t\t// this.setForm();\n\t\t\t// this.paramsForm['username']=\"\"\n\t\t\t// this.$refs.paramsForm.resetFields();\n\t\t\t// console.log(this.paramsForm)\n\t\t},\n\t\t// 添加书签\n\t\taddMark(){\n\t\t\tlet condition = JSON.stringify(this.getQueryParam());\n\t\t\tthis.$refs.collectComp.ajaxAdd(condition,1,this.collectParam.name,() => {\n\t\t\t\tthis.collectVisible = false;\n\t\t\t\tthis.$Message.success(\"添加书签成功\");\n\t\t\t})\n\t\t},\n\t\tnodeClickCollect(node){\n\t\t\tthis.curNode = node;\n\t\t\tgetCollectById(node.mark_id).then((res) => {\n\t\t\t\tthis.collectList = res.data.filter(list => list.mark_type === 1);\n\t\t\t})\n\t\t},\n\t\t// startCache(mark){\n\t\t// \tthis.curMark = mark;\n\t\t// \tthis.cacheVisible = true;\n\t\t// },\n\t\tgoCache(mark){\n\t\t\t// if(!this.cache_name.trim()){\n\t\t\t// \tthis.$Message.warning('请输入缓存名称');\n\t\t\t// \treturn;\n\t\t\t// }\n\t\t\tif(mark.is_cached){\n\t\t\t\treturn\n\t\t\t}\n\t\t\tthis.curMark = mark;\n\t\t\tthis.$refs.collectComp.ajaxEdit({is_cached:1},this.curMark,'缓存成功').then(() => {\n\t\t\t\tthis.nodeClickCollect(this.curNode);\n\t\t\t\t// this.cacheVisible = false;\n\t\t\t});\n\t\t},\n\t\tgoDownload(){\n\t\t\tlet str = `文件${new Date().getTime()}`;\n\t\t\tlet param = Object.assign({},this.getQueryParam(),{export_file:str});\n\t\t\tdownload(param).then((res) => {\n\t\t\t\tthis.$Message.success(\"请在任务导出界面查看\");\n\t\t\t})\n\t\t},\n\t\tcollect2Search(mark){\n\t\t\tlet tab = {\n\t\t\t\t...mark,\n\t\t\t\ttabId:mark.mark_id\n\t\t\t}\n\t\t\tthis.addTab(tab);\n\t\t\tthis.paramBack(JSON.parse(mark.mark_condations),async () => {\n\t\t\t\tthis.collectListVisible = false;\n\t\t\t\tthis.tableConfig.data = await this.getLogs(this.getQueryParam());\n\t\t\t});\n\t\t},\n\t\t// 设置options\n\t\tsetOptions(options,type){\n\t\t\tif(type === 'handler'){\n\t\t\t\treturn options.split(',');\n\t\t\t} else {\n\t\t\t\treturn options === 'country'?this.$store.state.countries:this.$store.state.cities;\n\t\t\t}\n\t\t},\n\t\t// 参数还原大法\n\t\tasync paramBack(param,callback){\n\t\t\t// 1. 还原单位 unit_id --> treeForm.unit\n\t\t\tthis.treeForm.unit = param.unit_id;\n\t\t\t// 2. 还原检索范围 判断有没有project_id  treeForm.type 没有0 有1\n\t\t\tthis.treeForm.type = param.project_id?1:0;\n\t\t\t// 3. 还原任务id project_id --> treeform.pro\n\t\t\tif(this.treeForm.type){\n\t\t\t\tthis.treeForm.pro = param.project_id;\n\t\t\t}\n\t\t\t// 4. 选择节点还原 analyse_id rulegroup_id rule_id 以最小为主  调用 ztree  根据参数找到treenode , 然后selectNode\n\t\t\tlet treeNode = null;\n\t\t\tlet idKey = null;\n\t\t\tlet temp = [param.analyse_id,param.rulegroup_id,param.rule_id].filter(val => !!val);\n\t\t\tlet temp2 = ['ana','rg','r'];\n\t\t\tidKey = temp2[temp.length - 1] + temp[temp.length - 1];\n\t\t\ttreeNode = this.$refs.rangeTree.RTree.getNodeByParam('idKey',idKey);\n\t\t\tthis.$refs.rangeTree.RTree.selectNode(treeNode);\n\t\t\tthis.selectNode = treeNode;\n\t\t\tgetFieldsByType(this.selectNode.idArr).then((res) => {\n\t\t\t\tthis.fields = res.filter(\n\t\t\t\t\t(list) => list.field_ename !== 'sourcelog'\n\t\t\t\t);\n\t\t\t\tthis.tableConfig.column = this.fields.map((list) => {\n\t\t\t\t\treturn {\n\t\t\t\t\t\ttitle: list.field_cname,\n\t\t\t\t\t\tkey: list.field_ename,\n\t\t\t\t\t};\n\t\t\t\t});\n\t\t\t\tthis.effectFields = this.fields.filter(list => param.hasOwnProperty(list.field_ename));\n\t\t\t\t// 5. 将剩余参数还原到 查询条件上\n\t\t\t\tthis.effectFields.forEach(field => {\n\t\t\t\t\tlet value;\n\t\t\t\t\tif(field.display_type === 'date' || field.display_type === 'time'){\n\t\t\t\t\t\tvalue = param[field.field_ename].split(';').map(val => new Date(val));\n\t\t\t\t\t} else {\n\t\t\t\t\t\tvalue = param[field.field_ename];\n\t\t\t\t\t}\n\t\t\t\t\tthis.$set(this.paramsForm,field.field_ename,value);\n\t\t\t\t})\n\t\t\t\tif(param.hasOwnProperty('req_ip')){\n\t\t\t\t\tthis.is_ip = param.is_ip;\n\t\t\t\t}\n\t\t\t\tthis.area = param.area;\n\t\t\t\tthis.qtype = param.qtype;\n\t\t\t\tif(callback){\n\t\t\t\t\tcallback();\n\t\t\t\t}\n\t\t\t})\n\t\t},\n\t\t// 删除标签\n\t\tremove(mark){\n\t\t\tlet that = this;\n            this.$Modal.confirm({\n                title:'是否删除该条数据',\n                onOk:function(){\n                    delCollect(mark.mark_id).then(() => {\n                        that.$Message.success('删除成功');\n                        that.nodeClickCollect(that.curNode);\n                    })\n                }\n            })\n\t\t}\n\t},\n};\n</script>\n<style lang=\"scss\">\n.search {\n\tbackground-color: #f3f3f4;\n\t.searchRange {\n\t\twidth: 300px;\n\t\tbackground-color: #2f4050;\n\t\t// background-color: #fff;\n\t\theight: 100%;\n\t\tpadding: 20px 10px;\n\t\tform {\n\t\t\t.ivu-form-item {\n\t\t\t\tcolor: #fff;\