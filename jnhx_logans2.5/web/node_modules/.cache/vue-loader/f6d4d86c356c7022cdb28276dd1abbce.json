   <span class=\"curPage\">{{ page_ }}</span>\n                /\n                <span class=\"totalPage\">{{ totalPage }}</span>\n            </li>\n            <li title=\"下一页\" @click=\"next\" :class=\"{'pageDisabled':page_ === totalPage || total_ === 0}\">\n                <a>\n                    <Icon type=\"md-arrow-round-forward\" />\n                </a>\n            </li>\n        </ul>\n    </div>\n</template>\n<script>\nexport default {\n    name: 'simple-page',\n    props:{\n        total:{\n            type:Number,\n            default:function(){\n                return 0;\n            }\n        },\n        page: {\n            type:Number,\n            default:function(){\n                return 1;\n            }\n        },\n        size: {\n            type:Number,\n            default:function(){\n                return 10;\n            }\n        }\n    },\n    data(){\n        return {\n            total_:0,\n            page_:1,\n            size_:10\n        }\n    },\n    watch: {\n        total(val) {\n            this.total_ = val;\n        },\n        page(val) {\n            this.page_ = val;\n        },\n        size(val) {\n            this.size_ = val;\n        },\n    },\n    computed: {\n        totalPage(){\n            return Math.ceil(this.total_ / this.size_);\n        }\n    },\n    methods: {\n        next() {\n            if(this.page_ !== this.totalPage && this.total_ !== 0){\n                this.page_ ++;\n                this.$emit('page-change', this.page_);\n            }\n        },\n        previous() {\n            if(this.page_ !== 1 && this.total_ !== 0){\n                this.page_ --;\n            }\n            this.$emit('page-change', this.page_);\n        }\n    },\n    created(){\n        this.total_ = this.total;\n        this.page_ = this.page;\n        this.size_ = this.size;\n    }\n}\n</script>\n<style lang=\"scss\">\n.simple-page {\n    height: 24px;\n    line-height: 24px;\n    ul {\n        li {\n            float: left;\n            a {\n                color: inherit;\n                padding: 0 10px;\n                i {\n                    font-size: 20px;\n                }\n                &:hover {\n                    color: #2d8cf0;\n                }\n            }\n        }\n        li.number {\n            font-size: 16px;\n            span {\n                margin: 0 10px;\n            }\n        }\n        li.pageDisabled {\n            cursor: not-allowed;\n            a {\n                cursor: not-allowed;\n                color: #ccc;\n            }\n        }\n    }\n}\n</style>"]}]}